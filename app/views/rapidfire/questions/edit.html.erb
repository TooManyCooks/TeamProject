<% content_for :title, "Edit Answers" %>

<% content_for :sideCol do %>
  <p id="notice"><%= notice %></p>
<% end %>


<% content_for :mainCol do %>
  <div id="center_box">
    <%= render partial: 'form',
    locals: { form: @question_form,
    url: question_group_question_path(@question_group, @question),
    method: :put,
    submit_text: 'Update Question' } %>
  </div>
<% end %>
